<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="jj.presentation.synthesizer.winforms" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
    <section name="jj.framework.persistence" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
  </configSections>

  <jj.presentation.synthesizer.winforms>
    <!--testPatchID="164"-->
    <testing
             testPatchID="176"
             mustShowInvisibleElements="false"
             mustCreateMockPatch="false"
             forceStateless="false"
             alwaysRecreateDiagram="true"
             toolTipsFeatureEnabled="true" />
    
    <filePaths sampleFilePath="Sample.wav"
               outputFilePath="WaveFileOutput.wav" />

    <memoryPersistence
      contextType="Memory"
      dialect=""
      location=""
      modelAssembly="JJ.Persistence.Synthesizer"
      mappingAssembly="JJ.Persistence.Synthesizer.Memory">
      <repositoryAssemblies>
        <repositoryAssembly>JJ.Persistence.Synthesizer.Memory</repositoryAssembly>
        <repositoryAssembly>JJ.Persistence.Synthesizer.DefaultRepositories</repositoryAssembly>
      </repositoryAssemblies>
    </memoryPersistence>
  </jj.presentation.synthesizer.winforms>
  
  <jj.framework.persistence
    contextType="NHibernate"
    dialect="SqlServer2008"
    location="Data Source=.\SQLEXPRESS;Initial Catalog=DEV_SynthesizerDB;User ID=dev;Password=dev;Persist Security Info=True"
    modelAssembly="JJ.Persistence.Synthesizer"
    mappingAssembly="JJ.Persistence.Synthesizer.NHibernate">
    <repositoryAssemblies>
      <repositoryAssembly>JJ.Persistence.Synthesizer.NHibernate</repositoryAssembly>
      <repositoryAssembly>JJ.Persistence.Synthesizer.DefaultRepositories</repositoryAssembly>
    </repositoryAssemblies>
  </jj.framework.persistence>

  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>