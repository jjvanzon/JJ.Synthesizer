<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="jj.framework.persistence" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
  </configSections>

  <appSettings>
    <add key="TestPatchID" value="164" />
    <add key="MustShowInvisibleElements" value="false" />
    <add key="MustCreateMockPatch" value="false"/>
  </appSettings>
  
  <jj.framework.persistence
    contextType="NHibernate"
    dialect="SqlServer2008"
    location="Data Source=.\SQLEXPRESS;Initial Catalog=DEV_SynthesizerDB;User ID=dev;Password=dev;Persist Security Info=True"
    modelAssembly="JJ.Persistence.Synthesizer"
    mappingAssembly="JJ.Persistence.Synthesizer.NHibernate">
    <repositoryAssemblies>
      <repositoryAssembly>JJ.Persistence.Synthesizer.NHibernate</repositoryAssembly>
      <repositoryAssembly>JJ.Persistence.Synthesizer.DefaultRepositories</repositoryAssembly>
    </repositoryAssemblies>
  </jj.framework.persistence>

  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>