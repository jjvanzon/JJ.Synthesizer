<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="jj.business.synthesizer.wishes"
		         type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
	</configSections>
	
	<jj.business.synthesizer.wishes	
		samplingRate="48000"
		speakers="Mono"
		audioFormat="Wav"
		interpolation="Line"
		audioLength="1"
		leadingSilence="0.25"
		trailingSilence="0.25"
		audioPlayBack="false"
		parallels="true"
		mathOptimization="true"
		diskCaching="true"
		longRunningTestCategory="Long">
		
		<nCrunch 
			samplingRate="150"
			samplingRateLongRunning="8"
			audioPlayBack="false" 
			impersonate="false"/>
		
		<azurePipelines
			samplingRate="1500"
			samplingRateLongRunning="100"
			audioPlayBack="false" 
			impersonate="false"/>
		
	</jj.business.synthesizer.wishes>

	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
			</dependentAssembly>
		</assemblyBinding>

	</runtime>
</configuration>