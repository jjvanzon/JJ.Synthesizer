<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
		<section name="jj.framework.persistence" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
		<section name="jj.business.synthesizer.tests" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
  </configSections>

  <jj.framework.persistence
    contextType="Memory"
    dialect=""
    location=""
    modelAssembly="JJ.Persistence.Synthesizer"
    mappingAssembly="JJ.Persistence.Synthesizer.Memory">
    <repositoryAssemblies>
      <repositoryAssembly>JJ.Persistence.Synthesizer.Memory</repositoryAssembly>
      <repositoryAssembly>JJ.Persistence.Synthesizer.DefaultRepositories</repositoryAssembly>
    </repositoryAssemblies>
  </jj.framework.persistence>

	<jj.business.synthesizer.tests 
		defaultOutputVolume="0.5"
		defaultOutputDuration="3" 
		defaultSamplingRate="48000"
		azurePipelinesSamplingRate="1500"
		azurePipelinesSamplingRateLongRunning="100"
		nCrunchSamplingRate="150"
		nCrunchSamplingRateLongRunning="50"
		longRunningTestCategory="Long" />
	
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
